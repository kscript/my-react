    "rollup-plugin-jsx": "^1.0.3",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-uglify": "^6.0.2",
    "tslib": "*",
    "typedoc": "^0.14.2",
    "typedoc-plugin-as-member-of": "^1.0.2",
    "typedoc-twilio-theme": "^1.0.0",
    "typescript": "^3.4.5",
    "uglify-es": "^3.3.9",
    "watchify": "^3.11.1"
